model Address {
  id         Int    @id @default(autoincrement())
  full_address String
  address String
  address2     String?
  district String // state, province, prefecture etc...
  city       String
  postal_code String
  country    String
  
  latitude Float?
  longtitude Float?

  dog_parks DogPark[]
  dog_friendly_dining DogFriendlyDining[]
}